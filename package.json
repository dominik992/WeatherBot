{
  "name": "wather-bot",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run api-docs && babel --out-dir ./build ./src",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf ./build",
    "lint": "eslint ./src",
    "start": "node ./build/www.js",
    "start:dev": "nodemon ./build/www.js --watch ./build/",
    "api-docs": "api-docs -f build/public/api-docs src",
    "test": "mocha -R spec --compilers js:babel-core/register \"src/**/*spec.js\"",
    "review": "npm prune && npm install && npm run build && npm test && npm run lint"
  },
  "dependencies": {
    "bluebird": "3.4.7",
    "body-parser": "1.15.0",
    "bunyan": "1.8.1",
    "cache-manager": "2.2.0",
    "cors": "2.7.1",
    "cross-env": "1.0.7",
    "debug": "2.2.0",
    "dotenv": "4.0.0",
    "env-cmd": "4.0.0",
    "express": "4.13.4",
    "lodash": "4.11.2",
    "messenger-bot": "2.4.0",
    "migrate": "0.2.3",
    "mime-types": "2.1.11",
    "mongoose": "4.4.14",
    "multer": "1.1.0",
    "newrelic": "1.37.2",
    "normalize-path": "2.0.1",
    "passport": "0.3.2",
    "request-promise": "4.1.1",
    "tar": "2.2.1",
    "temp": "0.8.3",
    "upath": "0.2.0",
    "urijs": "1.18.0",
    "validate": "3.0.1",
    "verror": "1.6.1"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015-node6": "0.4.0",
    "babel-preset-es2017": "6.22.0",
    "babel-preset-node6": "11.0.0",
    "babel-register": "6.8.0",
    "chai": "3.5.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "8.0.0",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.0.1",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "node-mocks-http": "1.5.2",
    "nodemon": "1.9.2",
    "request": "2.72.0",
    "rimraf": "2.5.2",
    "serve-static": "1.10.2",
    "sinon": "1.17.4",
    "chai-as-promised": "6.0.0"
  },
  "engines": {
    "node": ">=6.1.0"
  }
}
